{
  "name": "majsoul-plus-client",
  "version": "1.4.4",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron . --enable_logging",
    "pack": "npm run build",
    "build": "npm run build-win32 && npm run build-linux && npm run build-darwin",
    "build-win32": "electron-packager . Majsoul_Plus  --platform=win32 --arch=all --out ./build/unpacked --overwrite --icon=icons/icon.ico --ignore=build/",
    "build-linux": "electron-packager . Majsoul_Plus  --platform=linux --arch=all --out ./build/unpacked --overwrite --icon=icons/icon.png --ignore=build/",
    "build-darwin": "electron-packager . Majsoul_Plus  --platform=darwin --arch=all --out ./build/unpacked --overwrite --icon=icons/icon.icns --ignore=build/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamapig120/majsoul-custorm-server.git"
  },
  "author": "Handle",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/iamapig120/majsoul-custorm-server/issues"
  },
  "homepage": "https://github.com/iamapig120/majsoul-custorm-server#readme",
  "dependencies": {
    "adm-zip": "^0.4.13",
    "express": "^4.16.4",
    "tcp-ping": "^0.1.1"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "electron": "^4.0.1",
    "electron-packager": "^13.0.1"
  }
}
